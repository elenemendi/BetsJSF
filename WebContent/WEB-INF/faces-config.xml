<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>queryBean</managed-bean-name>
		<managed-bean-class>facade.QueryBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>createBean</managed-bean-name>
		<managed-bean-class>facade.CreateBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>ApplicationLauncher.xhtml</display-name>
		<from-view-id>/ApplicationLauncher.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>create</from-outcome>
			<to-view-id>/CreateQuestions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ApplicationLauncher.xhtml</display-name>
		<from-view-id>/ApplicationLauncher.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>query</from-outcome>
			<to-view-id>/QueryQuestions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateQuestions.xhtml</display-name>
		<from-view-id>/CreateQuestions.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>close</from-outcome>
			<to-view-id>/ApplicationLauncher.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QueryQuestions.xhtml</display-name>
		<from-view-id>/QueryQuestions.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>close</from-outcome>
			<to-view-id>/ApplicationLauncher.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ApplicationLauncher.xhtml</display-name>
		<from-view-id>/ApplicationLauncher.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>show</from-outcome>
			<to-view-id>/ShowAllEvents.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
